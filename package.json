{
  "name": "react-seed",
  "version": "1.0.0",
  "description": "react + redux + antd",
  "main": "./src/index.jsx",
  "scripts": {
    "lint": "eslint --fix --ext js --ext jsx src",
    "test": "npm run lint && cross-env NODE_ENV=test jest --config ./jest.config.json --coverage",
    "test:update": "jest -u",
    "build": "npm run test && webpack --config ./webpack-config/webpack.dev.js --mode development",
    "server": "webpack-dev-server --config ./webpack-config/webpack.dev.js --mode development",
    "start": "npm run build && npm run server",
    "production": "npm run clean && npm run test && webpack --config ./webpack-config/webpack.prod.js --mode production",
    "clean": "rimraf app-build reports coverage .nyc_output"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sylhero/react-seed"
  },
  "keywords": [
    "reactjs",
    "webpack",
    "redux",
    "antd"
  ],
  "author": "Yulong Song",
  "license": "ISC",
  "homepage": "https://github.com/sylhero/react-seed/README.md",
  "devDependencies": {
    "autoprefixer": "^8.1.0",
    "axios-mock-adapter": "^1.14.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-jest": "^22.4.1",
    "babel-loader": "^7.1.4",
    "babel-minify-webpack-plugin": "^0.3.0",
    "babel-plugin-import": "^1.6.6",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.10",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.1",
    "esformatter": "^0.10.0",
    "esformatter-jsx": "^8.0.1",
    "eslint": "^4.18.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "html-webpack-plugin": "^3.0.6",
    "image-webpack-loader": "^4.2.0",
    "jest": "^22.4.2",
    "node-sass": "^4.7.2",
